{
  "$schema": "https://raw.githubusercontent.com/DavidAnson/markdownlint/main/schema/markdownlint-config-schema.json",
  "default": true,
  // Disable line-length check for tables, code blocks
  "line-length": {
    "code_blocks": false,
    "tables": false
  },
  // Doesn't play nice with claude commands+skills and obsidian
  "first-line-heading": false
}
